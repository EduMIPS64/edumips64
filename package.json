{
  "name": "edumips64-webui",
  "version": "1.0.0",
  "description": "The web UI for EduMIPS64 (web.edumips.org). Depends on the web worker implementation of the EduMIPS64 core (worker.js).",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "build-dbg": "webpack -d",
    "deploy": "mkdirp build/gwt/war/edumips64 && copyfiles -f \"src/webapp/static/*\" build/gwt/war/edumips64",
    "serve": "serve build/gwt/war/edumips64/"
  },
  "keywords": [],
  "author": "Andrea Spadaccini",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "lodash": "^4.17.15",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-monaco-editor": "^0.36.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.0",
    "@babel/preset-env": "7.10.0",
    "@babel/preset-react": "7.10.0",
    "babel-loader": "8.1.0",
    "copyfiles": "2.3.0",
    "css-loader": "3.5.3",
    "file-loader": "6.0.0",
    "mkdirp": "1.0.4",
    "serve": "11.3.1",
    "style-loader": "1.2.1",
    "webpack": "5.0.0-beta.16",
    "webpack-cli": "3.3.11"
  }
