<module rename-to="edumips64">
  <inherits name="com.google.gwt.user.User"/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <inherits name="com.google.gwt.json.JSON" />
  <inherits name="com.google.gwt.webworker.WebWorker"/>
  <!-- Settings required for Web Worker support. A single permutation must be compiled. -->
  <set-property name="user.agent" value="safari" />
  <set-configuration-property name="user.agent.runtimeWarning" value="false" />
  <add-linker name="dedicatedworker" />

  <entry-point class="org.edumips64.client.WebUi"/>
  <source path="client" />
  <source path="core" />
  <source path="utils">
    <exclude name="io/Local*.java" />
    <exclude name="MetaInfo.java" />
    <exclude name="JavaPrefsConfigStore.java" />
  </source>
  <public path="client">
    <include name="index.html" />
    <include name="ui.js" />
    <include name="style.css" />
    <include name="logo.png" />
    <include name="favicon.png" />
  </public>
</module>
